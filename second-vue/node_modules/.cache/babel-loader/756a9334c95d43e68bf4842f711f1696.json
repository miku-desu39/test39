put>\r\n          </div>\r\n          <div class=\"resumeContent\">\r\n            <h3>实习经历</h3>\r\n            <el-input\r\n              type=\"textarea\"\r\n              :autosize=\"{ minRows: 3, maxRows: 3 }\"\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"resume.professionalAbility\"\r\n            >\r\n            </el-input>\r\n          </div>\r\n          <div class=\"resumeContent\">\r\n            <h3>工作经历</h3>\r\n            <el-input\r\n              type=\"textarea\"\r\n              :autosize=\"{ minRows: 3, maxRows: 3 }\"\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"resume.professionalAbility\"\r\n            >\r\n            </el-input>\r\n          </div>\r\n          <div class=\"resumeContent\">\r\n            <h3>获奖荣誉</h3>\r\n            <el-input\r\n              type=\"textarea\"\r\n              :autosize=\"{ minRows: 3, maxRows: 3 }\"\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"resume.professionalAbility\"\r\n            >\r\n            </el-input>\r\n          </div>\r\n\r\n          <div class=\"resumeContent\">\r\n            <h3>就职期望</h3>\r\n            <el-input\r\n              type=\"textarea\"\r\n              :autosize=\"{ minRows: 3, maxRows: 3 }\"\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"resume.professionalAbility\"\r\n            >\r\n            </el-input>\r\n          </div>\r\n\r\n          <el-button\r\n            type=\"primary\"\r\n            @click=\"saveInformation\"\r\n            style=\"width: 150px; margin-top: 30px\"\r\n            >保存</el-button\r\n          >\r\n        </el-main>\r\n      </el-tab-pane>\r\n      <el-tab-pane>\r\n        <span slot=\"label\" style=\"font-size: 15px\"\r\n          ><i class=\"el-icon-s-promotion\"></i> 投递记录</span\r\n        >\r\n\r\n        <el-main>\r\n          <el-table :data=\"postRecord\" border style=\"width: 100%\" stripe>\r\n            <el-table-column prop=\"jobName\" label=\"职位名\" 