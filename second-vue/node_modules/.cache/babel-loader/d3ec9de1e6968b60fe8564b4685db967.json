       <div style=\"padding-top: 20px\">\r\n          <span style=\"color: #999; font-size: 15px\"\r\n            >职位浏览量：{{ job.pageviews }}</span\r\n          >\r\n        </div>\r\n      </div>\r\n\r\n      <div>\r\n        <el-button type=\"primary\" @click=\"postJob\">投递简历</el-button>\r\n\r\n        <el-button type=\"warning\" @click=\"starJob\">收藏职位</el-button>\r\n      </div>\r\n    </div>\r\n\r\n    <div style=\"padding: 30px\">\r\n      <h4>一.公司介绍</h4>\r\n      <p>公司名：{{ job.companyName }}</p>\r\n      <p>公司描述：{{ job.companyIntroduce }}</p>\r\n\r\n      <h4>二.任职要求</h4>\r\n      <p>{{ job.requirement }}</p>\r\n\r\n      <h4>三.福利待遇</h4>\r\n      <!-- <el-input\r\n        type=\"textarea\"\r\n        :autosize=\"{ minRows: 2, maxRows: 4 }\"\r\n        placeholder=\"请输入内容\"\r\n        v-model=\"job.welfare\"\r\n      >\r\n      </el-input> -->\r\n      <div v-html=\"job.welfare\"></div>\r\n      <!-- <el-input\r\n        type=\"textarea\"\r\n        v-model=\"job.welfare\"\r\n        :autosize=\"{ minRows: 2, maxRows: 4 }\"\r\n        readonly=\"true\"\r\n        class=\"myInput\"\r\n      ></el-input> -->\r\n\r\n      <h4>四.工作城市</h4>\r\n      <p>{{ job.location }}</p>\r\n    </div>\r\n  </el-main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"JobDetail\",\r\n\r\n  data() {\r\n    return {\r\n      jobseekerId: 2,\r\n\r\n      jobId: \"\",\r\n\r\n      job: {\r\n        id: \"\",\r\n        companyName: \"\",\r\n        companyIntroduce: \"\",\r\n        name: \"\",\r\n        salary: \"\",\r\n        location: \"\",\r\n        tag1: \"\",\r\n        tag2: \"\",\r\n        tag3: \"\",\r\n        requirement: \"\",\r\n        welfare: \"\",\r\n        number: \"\",\r\n        pageviews: \"\",\r\n      },\r\n\r\n      email: window.sessionStorage.getItem(\"email\"),\r\n    };\r\n  },\r\n\r\n  created() {\r\n    if (this.$route.query.jobId) {\r\n      this.jobId = this.$route.query.jobId;\r\n    }\r\n\r\n    this.initJob();\r\n  },\r\n\r\n  methods: {\r\n    initJob() {\r\n      this.$message.success(this.jobId);\r\n\r\n      this.$axios\r\n        .get(\"/api/job/viewJobDetail?jobId=\" + this.jobId)\r\n        .then((resp) => {\r\n          if (resp.data.code === 200) {\r\n            this.job = resp.data.data;\r\n          } else {\r\n            this.$message.error(\"错误：\" + resp.data.message);\r\n          }\r\n        });\r\n    },\r\n\r\n    starJob() {\r\n      this.$axios\r\n        .post(\"/api/job/starJob\", {\r\n          jobId: this.jobId,\r\n          jobseekerId: this.jobseekerId,\r\n        })\r\n        .then((resp) => {\r\n          if (resp.data.code === 200) {\r\n            this.$message.success(\"收藏职位成功\");\r\n          } else {\r\n            this.$message.error(\"OH~ \" + resp.data.message);\r\n          }\r\n        });\r\n    },\r\n\r\n    postJob() {\r\n      this.$confirm(\"将投递该职位, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        this.$axios\r\n          .post(\"/api/job/postJob\", {\r\n            jobId: this.jobId,\r\n            jobseekerId: this.jobseekerId,\r\n          })\r\n          .then((resp) => {\r\n            if (resp.data.code === 200) {\r\n              this.$message.success(\"投递职位成功\");\r\n            } else {\r\n              this.$message.error(\"OH~ \" + resp.data.message);\r\n            }\r\n          });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.myHeader {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  background-color: white;\r\n  padding: 30px;\r\n}\r\n\r\n.job-tag-content {\r\n  padding-top: 6px;\r\n  font-size: 10px;\r\n  display: block;\r\n  height: 30px;\r\n  line-height: 30px;\r\n  margin-left: -43px;\r\n  color: #777;\r\n}\r\n.job-tag {\r\n  display: inline-block;\r\n  width: 61px;\r\n  height: 26px;\r\n  padding: 0 5px;\r\n  font-size: 12px;\r\n  line-height: 26px;\r\n  color: #999;\r\n  border: 1px solid #f0f0f0;\r\n  border-radius: 3px;\r\n  text-align: center;\r\n  margin-left: 6px;\r\n}\r\n</style>"],"sourceRoot":"src/component"}]}