BA;AAAA;;AAEA,WAAA,MAAA,CAAA,IAAA,CAAA,eAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,SAAA,CAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA,QAFA;AAGA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA;AAHA,OAAA,EAIA,IAJA,CAIA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA,EAAA,MAAA,CAAA,SAAA,CAAA,KAAA;AACA,UAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,UAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,MAAA,MAAA,CAAA,SAAA,CAAA,QAAA;AAEA,SANA,MAMA;AAEA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;AACA;AAEA,OAhBA,EAgBA,KAhBA,CAgBA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OArBA;AAuBA,KAzCA;AA2CA,IAAA,OA3CA,qBA2CA;AAAA;;AAEA,WAAA,MAAA,CAAA,IAAA,CAAA,WAAA,UAAA,EAAA,KAAA,YAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AAEA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;;AAEA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,OATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,OAXA;AAcA;AA3DA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app\">\r\n    <div class=\"main\">\r\n\r\n<el-container>\r\n  <el-header>\r\n    <el-tabs value=\"first\">\r\n      <el-tab-pane label=\"账号登陆\" name=\"first\">\r\n        <el-form ref=\"loginForm\" :model=\"loginForm\" label-width=\"80px\">\r\n          <el-form-item label=\"类型\">\r\n            <el-radio v-model=\"loginForm.userType\" label=\"Company\">公司</el-radio>\r\n            <el-radio v-model=\"loginForm.userType\" label=\"JobSeeker\">求职者</el-radio>\r\n          </el-form-item>\r\n          <el-form-item label=\"账号\">\r\n            <el-input v-model=\"loginForm.email\" placeholder=\"邮箱/账号/手机号\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\">\r\n            <el-input v-model=\"loginForm.password\" placeholder=\"请输入密码\"  show-password></el-input>\r\n          </el-form-item>\r\n\r\n        </el-form>\r\n\r\n        <div class=\"customFooter\">\r\n          <el-button type=\"primary\" @click=\"login\">登录</el-button>\r\n          <el-button type=\"primary\" @click=\"dialogVisible = true\">注册</el-button>\r\n        </div>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"手机短信登录\" name=\"second\">配置</el-tab-pane>\r\n      <el-tab-pane label=\"微信登录\" name=\"third\">角色</el-tab-pane>\r\n    </el-tabs>\r\n  </el-header>\r\n</el-container>\r\n\r\n      <el-dialog\r\n          title=\"注册\"\r\n          :visible.sync=\"dialogVisible\"\r\n          width=\"30%\" center>\r\n        <el-form ref=\"registerForm\" :model=\"registerForm\" label-width=\"80px\">\r\n          <el-form-item label=\"类型\">\r\n            <el-radio v-model=\"registerForm.newUserType\" label=\"Company\">公司</el-radio>\r\n            <el-radio v-model=\"registerForm.newUserType\" label=\"JobSeeker\">求职者</el-radio>\r\n          </el-form-item>\r\n          <el-form-item label=\"邮箱账号\">\r\n            <el-input v-model=\"registerForm.newEmail\"></el-input>\r\n            <el-button @click=\"sendCode\" >发送验证码</el-button>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\">\r\n            <el-input v-model=\"registerForm.newPassword\" show-password></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"验证码\">\r\n            <el-input v-model=\"registerForm.code\"></el-input>\r\n          </el-form-item>\r\n\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n  </div>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Home\",\r\n\r\n  data() {\r\n    return {\r\n\r\n      registerForm:{\r\n        newUserType:'',\r\n        newEmail:'',\r\n        newPassword:'',\r\n        code:'',\r\n      },\r\n\r\n      loginForm:{\r\n        userType:'',\r\n        email:'',\r\n        password:'',\r\n      },\r\n\r\n      dialogVisible:false,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n\r\n    sendCode(){\r\n\r\n      this.$axios.get(\"/Login/sendCode?email=\"+this.registerForm.newEmail).then((resp)=>{\r\n        if(resp.data.code === 200){\r\n          this.$message.success(\"发送成功！\");\r\n        }else{\r\n          this.$message.error(\"发送失败！\");\r\n        }\r\n\r\n      })\r\n\r\n\r\n    },\r\n\r\n    login() {\r\n\r\n      this.$axios.post(\"/Login/submit\",{\r\n        email:this.loginForm.email,\r\n        password:this.loginForm.password,\r\n        userType:this.loginForm.userType\r\n      }).then((resp) => {\r\n        if(resp.data.code === 200){\r\n          window.sessionStorage.setItem(\"email\", this.loginForm.email);\r\n          window.sessionStorage.setItem(\"userType\", this.loginForm.userType);\r\n          this.$message.success(\"登录成功！\");\r\n          this.$router.push('/'+this.loginForm.userType);\r\n\r\n        }else{\r\n\r\n          this.$message.error(\"登录失败！\");\r\n        }\r\n\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'error',\r\n          message: '登录失败'\r\n        });\r\n      })\r\n\r\n    },\r\n\r\n    addUser(){\r\n\r\n      this.$axios.post(\"/Login\"+\"/addUser\", this.registerForm).then((resp) => {\r\n\r\n        if(resp.data.code === 200){\r\n          this.$message.success(\"注册成功\");\r\n        }else {\r\n          this.$message.error(\"注册失败\");\r\n        }\r\n\r\n        this.dialogVisible = false;\r\n      }).catch((error)=>{\r\n        this.$message.error(\"注册失败\");\r\n      })\r\n\r\n\r\n    }\r\n\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app{\r\n  display: flex;\r\n  justify-content:center;\r\n  align-items:center;\r\n\r\n  width: 100%;height: 100%;\r\n  background-image: url(../img/bg3.jpg);\r\n  background-size: cover; /* 使图片平铺满整个浏览器（从宽和高的最大需求方面来满足，会使某些部分无法显示在区域中） */\r\n  position: absolute; /* 不可缺少 */\r\n  z-index: -1;\r\n  background-repeat: no-repeat;\r\n}\r\n\r\n.main{\r\n  background-color: white;\r\n  width: 20%;\r\n  height: 40%;\r\n}\r\n\r\n.customFooter{\r\n\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}