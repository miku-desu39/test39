item>\r\n        <el-menu-item index=\"4\">我的精品</el-menu-item>\r\n        <el-menu-item index=\"5\">双选会</el-menu-item>\r\n        <el-menu-item index=\"6\">论文管理</el-menu-item>\r\n      </el-menu>\r\n\r\n      <el-dropdown @command=\"quit\" style=\"margin-right: 20px\">\r\n  <span class=\"el-dropdown-link\">\r\n    welcome! {{this.uid}}<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n  </span>\r\n        <el-dropdown-menu slot=\"dropdown\">\r\n          <el-dropdown-item command=\"a\">退出登录</el-dropdown-item>\r\n        </el-dropdown-menu>\r\n      </el-dropdown>\r\n    </el-header>\r\n\r\n    <el-main style=\"padding-top: 0px\">\r\n\r\n      <el-container>\r\n        <el-header class=\"myHeader\">\r\n          <span>置顶课程</span>\r\n          <el-button type=\"primary\" @click=\"dialogVisible=true\">新建课程</el-button>\r\n        </el-header>\r\n        <el-divider class=\"myDivider\"></el-divider>\r\n        <el-main>\r\n          <div v-for=\"(item, index) in this.course\":key=\"item.courseName\" class=\"courseDiv\">\r\n            <div class=\"courseDivP1\">\r\n              <div style=\"margin: 10px\"><strong><span style=\"font-size: 20px; color: white\"><a @click=\"courseDetail(index)\">{{ item.courseName }}</a></span></strong></div>\r\n              <div style=\"margin: 10px\"><span style=\"font-size: 15px; color: white\">加课码：{{item.courseId}}</span></div>\r\n            </div>\r\n\r\n            <div class=\"courseDivP2\">\r\n              <div style=\"height: 75%\">\r\n                <div><span style=\"font-size: 15px;\">近期作业</span></div>\r\n                <div style=\"margin-top: 5px\"><span>Homework{{index+1}}</span></div>\r\n                <div style=\"margin-top: 5px\"><span>Homework{{index+2}}</span></div>\r\n              </div>\r\n              <el-divider class=\"myDivider\"></el-divider>\r\n              <div style=\"display: flex; flex-direction: row-reverse\"><el-button @click=\"deleteCourse(index)\" size=\"small\">删除</el-button></div>\r\n            </div>\r\n\r\n          </div>\r\n          <el-divider></el-divider>\r\n        </el-main>\r\n      </el-container>\r\n\r\n    </el-main>\r\n\r\n    <el-dialog\r\n        title=\"创建新课程\"\r\n        :visible.sync=\"dialogVisible\"\r\n        width=\"30%\" center>\r\n      <el-form ref=\"newCourseForm\" :model=\"newCourseForm\" label-width=\"80px\">\r\n        <el-form-item label=\"加课码\">\r\n          <el-input v-model=\"newCourseForm.courseId\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"课程名\">\r\n          <el-input v-model=\"newCourseForm.courseName\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addCourse\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </el-container>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Teacher\",\r\n  data() {\r\n    return {\r\n\r\n      uid:window.sessionStorage.getItem(\"uid\"),\r\n\r\n      course:[],\r\n\r\n      dialogVisible:false,\r\n\r\n      newCourseForm:{\r\n        courseId:'',\r\n        courseName:'',\r\n        courseTeacher:'',\r\n      },\r\n\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.findAllCourse();\r\n  },\r\n\r\n  watch:{\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    findAllCourse(){\r\n      this.$axios.get(\"/Teacher/getCourse?uid=\"+this.uid).then((resp)=>{\r\n        this.course = resp.data;\r\n      })\r\n    },\r\n\r\n\r\n\r\n    addCourse(){\r\n      this.newCourseForm.courseTeacher=this.uid;\r\n      this.$axios.post(\"/Teacher/addCourse\", this.newCourseForm).then((resp)=>{\r\n        this.dialogVisible=false;\r\n        this.$message.success(\"添加成功\");\r\n        this.findAllCourse();\r\n      })\r\n    },\r\n\r\n    deleteCourse(index){\r\n      this.$confirm('此操作将永久删除课程, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(()=>{\r\n        this.$axios.get(\"/Teacher/deleteCourse?courseId=\"+this.course[index].courseId).then((resp)=>{\r\n          this.$message.success(\"删除成功\");\r\n          this.findAllCourse();\r\n        })\r\n      })\r\n    },\r\n\r\n    courseDetail(index){\r\n      window.sessionStorage.setItem(\"courseInfo\", JSON.stringify(this.course[index]));\r\n      this.$router.push('/Course');\r\n\r\n    },\r\n\r\n    quit(command){\r\n      this.$router.replace('/');\r\n      this.$message.success(\"退出成功！\");\r\n    },\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.courseDiv{\r\n  height: 273px;\r\n  display: inline-block;\r\n  margin-right: 40px;\r\n  width: 270px;\r\n  background: #fff;\r\n  border: 1px solid #E2E6ED;\r\n  border-radius: 8px;\r\n}\r\n\r\n.myHeader{\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.courseDivP1{\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 50%;\r\n  background-size: cover!important;\r\n  background-image: url(\"../img/courseDiv0.png\");\r\n}\r\n\r\n.courseDivP2{\r\n  height: 50%;\r\n}\r\n\r\n\r\n.myDivider{\r\n  margin-top: 0px;\r\n  margin-bottom: 0px;\r\n}\r\n\r\n\r\n</style>"]}]}